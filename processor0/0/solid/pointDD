FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0/solid";
    object      pointDD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    bottom
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    interface
    {
        type            calculated;
    }
    procBoundary0to1
    {
        type            processor;
    }
    procBoundary0to2
    {
        type            processor;
    }
    procBoundary0to3
    {
        type            processor;
    }
    procBoundary0to4
    {
        type            processor;
    }
}


