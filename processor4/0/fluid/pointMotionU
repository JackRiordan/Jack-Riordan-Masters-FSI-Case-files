FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0/fluid";
    object      pointMotionU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            fixedValue;
        value           nonuniform List<vector> 0();
    }
    right
    {
        type            slip;
    }
    left
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    bottom
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    interface
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    procBoundary4to0
    {
        type            processor;
    }
    procBoundary4to1
    {
        type            processor;
    }
    procBoundary4to2
    {
        type            processor;
    }
    procBoundary4to5
    {
        type            processor;
    }
    procBoundary4to6
    {
        type            processor;
    }
}


